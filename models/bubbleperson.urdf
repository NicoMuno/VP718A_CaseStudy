<?xml version="1.0"?>
<robot name="bubble_man">
  
  <!-- Base link: Torso (main body) -->
  <link name="torso">
    <visual>
      <geometry>
        <sphere radius="0.25"/>
      </geometry>
      <material name="torso_color">
        <color rgba="0.8 0.4 0.2 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <sphere radius="0.25"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="20.0"/>
      <inertia ixx="0.5" ixy="0.0" ixz="0.0" iyy="0.5" iyz="0.0" izz="0.5"/>
    </inertial>
  </link>

  <!-- Head -->
  <link name="head">
    <visual>
      <geometry>
        <sphere radius="0.12"/>
      </geometry>
      <material name="head_color">
        <color rgba="1.0 0.8 0.6 1.0"/>
